 pantalla+=1; 
}
}

void juego4(){

        println(ironman.hp);
  println(ironman2.hp);
  if (keyPressed){
if (key== 'r'){
  ironman.dibujar();
 ironman.hp = ironman.hp - 30; 
     textSize(10);
fill(255);
text("intenta otra vez", 200, 250);
}
if (key== 'q'){

 ironman2.hp = ironman2.hp - 30; 
}
 if (key== 't'){
   ironman.dibujar();
 ironman.hp = ironman.hp - 50; 
       textSize(10);
fill(255);
text("ups, ya rompiste el programa", 200, 250);
}
 if (key== 'w'){
 ironman2.hp = ironman2.hp - 30; 
 }
if (key== 'y'){
  ironman.dibujar();
       textSize(10);
fill(255);
text("correcto, ganaste presiona 1 para regresar", 200, 250);
 ironman.hp = 0; 
}
 if (key== 'e'){
 ironman2.hp = ironman2.hp - 20; 
 }
if (ironman.hp<=0){
 pantalla+=1; 
}
}
}
void juego5(){
          println(ironman.hp);
  println(superman.hp);
  if (keyPressed){
if (key== 'i'){
  superman.dibujar();
 ironman.hp = ironman.hp - 40; 
}
if (key== 'q'){

 superman.hp = superman.hp - 30; 
}
 if (key== 'o'){
   superman.dibujar();
 ironman.hp = ironman.hp - 40; 
}
 if (key== 'w'){
 superman.hp = superman.hp - 30; 
 }
if (key== 'p'){
  superman.dibujar();

 ironman.hp = ironman.hp - 50; 
}
 if (key== 'e'){
 superman.hp = superman.hp - 20; 
 }
if (ironman.hp<=0){
 pantalla+=1; 
}
}
}

//--------------------------------------------------------------------------------
void resultado1(){
  if(batman.hp==0){
   ironman.dibujar();
 }else{
   batman.dibujar();
 } 
}
void resultado2(){
  if(deadpool.hp==0){
   ironman.dibujar();
 }else{
   deadpool.dibujar();
 } 
}
//---------------------------------------------------------------------------------
class Heroe {
  String nombre;
  int hp;
  int ataque1;
  int ataque2 ;
  int movimiento;
  int dibujo;
  
  Heroe(String nombre_, int hp_, int ataque1_, int ataque2_, int movimiento_ ,  int dibujo_){
  nombre = nombre_;
  hp = hp_;
  ataque1 = ataque1_;
  ataque2 = ataque2_;
  movimiento = movimiento_;
  dibujo = dibujo_;
}

void juego(){
  
}


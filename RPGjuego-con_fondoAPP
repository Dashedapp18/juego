Heroe batman;
Heroe ironman;
Heroe ironman2;
Heroe deadpool;
Heroe alvaro;
Heroe superman;
int pantalla = 0;
PImage inicio;
PImage seleccion;
PImage fondo;
PImage resultado;
int turno = 0;

void setup(){
  background(0);
  size(768,368);
    inicio = loadImage("marveldc.jpg");
    seleccion = loadImage("select.jpg");
    fondo = loadImage("gotica.png");
    resultado = loadImage("resultado.jpg");
      batman = new Heroe("Batman", 100,25,20,40,0);
  deadpool = new Heroe ("Deadpool",100, 30, 30, 50,1);
   alvaro = new Heroe ("Alvaro", 100, 30, 50, 60, 2);
  ironman2 = new Heroe("Ironman2", 100,30,20,30,3);
  ironman = new Heroe("Ironman", 100,30,20,30,3);
 superman = new Heroe ("Superman", 100, 40, 40, 50,4);

}

void draw(){

    switch(pantalla){ 
    case 0:
   // image(inicio, 0, 0);
   background(inicio);
     if (mousePressed == false){
    }else{
    pantalla +=1;
    }
    break;
    case 1:
   background(seleccion);
      fill(#6f6f6f);
 seleccion();
   
    break;
    case 2:
    background(fondo);
     if (key == 'z' || key == 'Z'){
      batman.dibujar(); 
   textSize(15);
fill(255);
text("encuentra el boton de poder supremo aleatorio elige sabiamente: a,s,d", 50, 50);
juego1();
  } 
  juego1();
 
     if (key == 'x' || key == 'X'){
     deadpool.dibujar();
          textSize(15);
fill(255);
text("encuentra el boton de poder supremo aleatorio elige sabiamente: f,g,h", 50, 50);
juego2();
   }  
   juego2(); 
      if (key == 'c' || key == 'C'){
     alvaro.dibujar();
            textSize(7);
fill(255);
text("encuentra el boton de poder supremo aleatorio elige sabiamente para acabar con el hombre invisible: j,k,l", 20, 20);
juego3();
   }
   juego3();
      if (key == 'v' || key == 'V'){
     ironman.dibujar();
              textSize(15);
fill(255);
text("encuentra el boton de poder supremo aleatorio elige sabiamente para matarte a ti mismo: r,t,y", 50, 50);
juego4();
   }
  juego4();
      if (key == 'b' || key == 'B'){
     superman.dibujar();
                   textSize(12);
fill(255);
text("encuentra el boton de poder supremo aleatorio elige sabiamente para defender a la DICIS del no tener papel higienico: i,o,p", 20, 40);
juego5();
   }
   juego5();
      pushMatrix();
      translate(540,20);
      ironman.dibujar();
      popMatrix();

    break;
    case 3:
    //resolucion
    background(resultado);
     if(keyPressed){
      if (key == '1'){
    pantalla=0;
      }
     }
    break;
    }

}   
//----------------------------------------------------------------------------------
void seleccion(){
  rect(50,230,100,100);
textSize(25);
fill(0);
text("Batman", 55, 250); 
  textSize(15); 
   text("Presiona Z", 55, 300); 
  if (keyPressed){
   if (key == 'z' || key == 'Z'){
     pantalla +=1; 
   }
  }
       fill(#ff0000);
   rect(175,230,100,100);
   textSize(20);
fill(0);
text("Deadpool", 180, 250); 
     textSize(15); 
   text("Presiona X", 180, 300); 
     if (keyPressed){
   if (key == 'x' || key == 'X'){
     pantalla +=1;
    
   }
  }
          fill(255);
   rect(290,230,100,100);
   textSize(20);
fill(0);
text("Alvaro", 295, 250); 
  textSize(15); 
   text("Presiona C", 295, 300); 
     if (keyPressed){
   if (key == 'c' || key == 'C'){
     pantalla +=1;
   }
  }
   
             fill(#801815);
   rect(420,230,100,100);
   textSize(20);
fill(0);
text("Ironman", 425, 250); 
     textSize(15); 
   text("Presiona V", 425, 300); 
     if (keyPressed){
   if (key == 'v' || key == 'V'){
     ironman.dibujar();
     pantalla +=1;
   }
  }
   
                fill(#ffff00);
   rect(550,230,100,100);
   textSize(20);
fill(0);
text("Superman", 553, 250); 
     textSize(15); 
   text("Presiona B", 553, 300); 
     if (keyPressed){
   if (key == 'b' || key == 'B'){
     pantalla +=1;
     superman.dibujar();
   }
  }

}

void juego1(){
//ironman.hp=100;
//batman.hp=100;
 
println(ironman.hp);
  println(batman.hp);
//if (keyPressed){
if (key== 'a'|| key== 'A'){
  batman.dibujar();
 ironman.hp = 100 - 25; 
    textSize(10);
fill(255);
text("intenta otra vez", 200, 250); 
}
if (key== 'q'){
  //batman.dibujar();

 batman.hp = 100 - 30; 
}
 if (key== 's'){
   batman.dibujar();
 ironman.hp = 100 - 20; 
    textSize(10);
fill(255);
text("ups, ya rompiste el programa", 200, 250); 
}
 if (key== 'w'){
  // batman.dibujar();
 batman.hp = 100 - 30; 
 }
if (key== 'd'){
  batman.dibujar();
 ironman.hp = 0;
    textSize(10);
fill(255);
text("correcto, ganaste presiona 1 para regresar", 200, 250); 
pantalla+=1;
}
 if (key== 'e'){
   //batman.dibujar();
 batman.hp = 100 - 20; 
 }
else if (ironman.hp<=0){
 pantalla+=1; 
}
}
//}
